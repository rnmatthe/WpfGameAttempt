<Window x:Class="WpfGameAttempt.MainMenu"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfGameAttempt"
        mc:Ignorable="d"
        Title="MainMenu" Height="600" Width="1000">
    <Grid>
        <TextBlock x:Name="MyTextBlock" HorizontalAlignment="Center" Margin="0,20,0,0" TextWrapping="Wrap" Text="TextBlock" VerticalAlignment="Top" Width="900" FontSize="16"/>
        <TextBox x:Name="InputTextBox" KeyDown="key_down" HorizontalAlignment="Center" Height="23" Margin="0,0,0,20" TextWrapping="Wrap" VerticalAlignment="Bottom" Width="900" FontSize="16"/>

    </Grid>
</Window>
